
public class InsertionSort {
	
	public int[] insertionSort(int[] ar){
        for(int i = 1; i < ar.length; i++){
            int index = ar[i];
            int j = i;
            while(j>0 && ar[j-1]> index){
                ar[j] = ar[j-1];
                j--;
            }
            ar[j] = index;
        }
        	return ar;
		}
	public void print(int[] ar)
	{
		for(int i=0; i<50; i++){
			System.out.println(ar[i]);
		}
	}
	public int[] reverse(int[] ar){
		int[] newAr = new int[50];
		for(int i=49; i >=0; i--){
			newAr[49-i] = ar[i];
		}
		return newAr;
	}
	}
        
    
	

        
